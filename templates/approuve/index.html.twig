{% extends 'base.html.twig' %}

{% block title %}Approuve index{% endblock %}

{% block body %}
    <h1>Approuve index</h1>

    <table class="table">
        <thead>
        <tr>
            <th>Id</th>
            <th>actions</th>
        </tr>
        </thead>
        <tbody>
        {% for approuve in approuves %}
            <tr>
                <td>{{ approuve.id }}</td>
                <td>
                    <a href="{{ path('approuve_show', {'id': approuve.id}) }}">show</a>
                    <a href="{{ path('approuve_edit', {'id': approuve.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="2">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('approuve_new') }}">Create new</a>
{% endblock %}
