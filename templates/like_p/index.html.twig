{% extends 'base.html.twig' %}

{% block title %}LikeP index{% endblock %}

{% block body %}
    <h1>LikeP index</h1>

    <table class="table">
        <thead>
        <tr>
            <th>Id</th>
            <th>actions</th>
        </tr>
        </thead>
        <tbody>
        {% for like_p in like_ps %}
            <tr>
                <td>{{ like_p.id }}</td>
                <td>
                    <a href="{{ path('like_p_show', {'id': like_p.id}) }}">show</a>
                    <a href="{{ path('like_p_edit', {'id': like_p.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="2">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('like_p_new') }}">Create new</a>
{% endblock %}
