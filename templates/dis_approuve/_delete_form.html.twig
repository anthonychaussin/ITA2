<form method="post" action="{{ path('dis_approuve_delete', {'id': dis_approuve.id}) }}"
      onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ dis_approuve.id) }}">
    <button class="btn">Delete</button>
</form>
